@{
    ViewBag.Title = "Group Lists";
}

<script src="~/Scripts/jquery-1.10.2.js"></script>

<div class="container">

    <div class="row">
        <div class="col-lg-12">

            <!-- Categories Well -->
            <div class="well">
                <h4 style="font-weight:bold">All Questions</h4>
                <div class="row">
                    <div class="col-lg-6">
                        <ul class="list-unstyled">
                            @for (int a = 0; a < ViewBag.z.Count; a++)
                            {
                                if (a >= ViewBag.z.Count || a < 0)
                                {
                                    break;
                                }
                                <li>
                                    <a href="../User/post?pid=@ViewBag.z[a].postId">@ViewBag.z[a].title.ToString()</a>
                                </li>
                            }
                        </ul>
                    </div>

                </div>
                <!-- /.row -->
            </div>
        </div>
    </div>
</div>